<Window x:Class="AORViewer.Views.AreaAddUserWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:AORViewer.Views"
		mc:Ignorable="d"
		Title="Add User Window" Height="270" Width="270">
	<Grid>
		<Label Content="Users" HorizontalAlignment="Left" Margin="10,16,0,0" VerticalAlignment="Top"/>
		<ListBox ItemsSource="{Binding Users}" HorizontalAlignment="Left" Height="140" Margin="10,47,0,0" VerticalAlignment="Top" Width="242">
			<ListBox.ItemTemplate>
				<DataTemplate>
					<TextBlock Text="{Binding Username}" />
				</DataTemplate>
			</ListBox.ItemTemplate>
		</ListBox>
		<Button Content="Add" HorizontalAlignment="Left" Margin="177,201,0,0" VerticalAlignment="Top" Width="75"/>

	</Grid>
</Window>
